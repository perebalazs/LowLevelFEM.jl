\begin_layout Standard
\begin_inset Formula 
\[
\ten{\sigma}\cdot\nabla+\vec{f}=\vec{0}\qquad/\cdot\vec{v}\qquad/\intop_{\left(v\right)}\dots dv
\]

\end_inset


\begin_inset Formula 
\[
-W\left(\vec{r},\vec{v}\right)=\intop_{\left(v\right)}\vec{v}\cdot\overset{\downarrow}{\ten{\sigma}}\cdot\nabla dv+\intop_{\left(v\right)}\vec{v}\cdot\vec{f}dv=0
\]

\end_inset


\begin_inset Formula 
\[
-W\left(\vec{r},\vec{v}\right)=\intop_{\left(v\right)}\overset{\downarrow}{\overline{\vec{v}\cdot\ten{\sigma}}}\cdot\nabla dv-\intop_{\left(v\right)}\overset{\downarrow}{\vec{v}}\cdot\ten{\sigma}\cdot\nabla dv+\intop_{\left(v\right)}\vec{v}\cdot\vec{f}dv=0
\]

\end_inset


\begin_inset Formula 
\[
-W\left(\vec{r},\vec{v}\right)=\intop_{\left(a\right)}\vec{v}\cdot\ten{\sigma}\cdot\vec{n}da-\intop_{\left(v\right)}\overset{\downarrow}{\vec{v}}\cdot\ten{\sigma}\cdot\nabla dv+\intop_{\left(v\right)}\vec{v}\cdot\vec{f}dv=0
\]

\end_inset


\begin_inset Formula 
\[
\ten{\sigma}\cdot\vec{n}=\vec{t}\qquad\vec{r}\in a_{t}
\]

\end_inset


\begin_inset Formula 
\[
-W\left(\vec{r},\vec{v}\right)=\intop_{\left(a_{t}\right)}\vec{v}\cdot\vec{t}da-\intop_{\left(v\right)}\overset{\downarrow}{\vec{v}}\cdot\ten{\sigma}\cdot\nabla dv+\intop_{\left(v\right)}\vec{v}\cdot\vec{f}dv=0
\]

\end_inset


\begin_inset Formula 
\[
W\left(\vec{r},\vec{v}\right)=\intop_{\left(v\right)}\overset{\downarrow}{\vec{v}}\cdot\ten{\sigma}\cdot\nabla dv-\intop_{\left(a_{t}\right)}\vec{v}\cdot\vec{t}da-\intop_{\left(v\right)}\vec{v}\cdot\vec{f}dv=0
\]

\end_inset


\begin_inset Formula 
\[
\vec{t}\to\mat t=\begin{bmatrix}t_{x}\\
t_{y}\\
t_{z}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\vec{f}\to\mat f=\begin{bmatrix}f_{x}\\
f_{y}\\
f_{z}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\vec{v}\to\mat v=\begin{bmatrix}v_{x}\\
v_{y}\\
v_{z}
\end{bmatrix}=\begin{bmatrix}\sum_{i=1}^{N}h_{i}\left(\xi,\eta,\zeta\right)\phi_{xi}\\
\sum_{i=1}^{N}h_{i}\left(\xi,\eta,\zeta\right)\phi_{yi}\\
\sum_{i=1}^{N}h_{i}\left(\xi,\eta,\zeta\right)\phi_{zi}
\end{bmatrix}=
\]

\end_inset


\begin_inset Formula 
\[
=\underbrace{\begin{bmatrix}h_{1} & 0 & 0 & h_{2} & 0 & 0 & \cdots & h_{N} & 0 & 0\\
0 & h_{1} & 0 & 0 & h_{2} & 0 & \cdots & 0 & h_{N} & 0\\
0 & 0 & h_{1} & 0 & 0 & h_{2} & \cdots & 0 & 0 & h_{N}
\end{bmatrix}}_{\mat H\left(\xi,\eta,\zeta\right)}\underbrace{\begin{bmatrix}\phi_{x1}\\
\phi_{y1}\\
\phi_{z1}\\
\phi_{x2}\\
\phi_{y2}\\
\phi_{z2}\\
\vdots\\
\phi_{xN}\\
\phi_{yN}\\
\phi_{zN}
\end{bmatrix}}_{\mat{\phi}}=\mat H\left(\xi,\eta,\zeta\right)\mat{\phi}
\]

\end_inset


\begin_inset Formula 
\[
\ten{\sigma}\to\mat s=\begin{bmatrix}\sigma_{x} & \tau_{xy} & \tau_{xz}\\
\tau_{yx} & \sigma_{y} & \tau_{yz}\\
\tau_{zx} & \tau_{zy} & \sigma_{z}
\end{bmatrix}
\]

\end_inset

Ha 
\begin_inset Formula $\ten{\sigma}=\ten{\sigma}^{T}$
\end_inset


\begin_inset Formula 
\[
\ten{\sigma}\to\mat s=\begin{bmatrix}\sigma_{x} & \tau_{xy} & \tau_{zx}\\
\tau_{xy} & \sigma_{y} & \tau_{yz}\\
\tau_{zx} & \tau_{yz} & \sigma_{z}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\ten{\nabla}\to\mat{\partial}=\begin{bmatrix}\parc{}x\\
\parc{}y\\
\parc{}z
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
W\left(\vec{r},\vec{v}\right)=\intop_{\left(v\right)}\mat{\phi}^{T}\overset{\downarrow}{\mat H^{T}}\ten s\mat{\partial}dv-\intop_{\left(a_{t}\right)}\mat{\phi}^{T}\mat H^{T}\mat tda-\intop_{\left(v\right)}\mat{\phi}^{T}\mat H^{T}\mat fdv=0
\]

\end_inset


\begin_inset Formula 
\[
\overset{\downarrow}{\mat H^{T}}\ten s\mat{\partial}=\underbrace{\begin{bmatrix}h_{1} & 0 & 0\\
0 & h_{1} & 0\\
0 & 0 & h_{1}\\
h_{2} & 0 & 0\\
0 & h_{2} & 0\\
0 & 0 & h_{2}\\
\vdots & \vdots & \vdots\\
h_{N} & 0 & 0\\
0 & h_{N} & 0\\
0 & 0 & h_{N}
\end{bmatrix}}_{N\times3}\underbrace{\begin{bmatrix}\sigma_{x} & \tau_{xy} & \tau_{zx}\\
\tau_{xy} & \sigma_{y} & \tau_{yz}\\
\tau_{zx} & \tau_{yz} & \sigma_{z}
\end{bmatrix}}_{3\times3}\underbrace{\begin{bmatrix}\parc{}x\\
\parc{}y\\
\parc{}z
\end{bmatrix}}_{3\times1}=
\]

\end_inset


\begin_inset Formula 
\[
=\underbrace{\begin{bmatrix}h_{1} & 0 & 0\\
0 & h_{1} & 0\\
0 & 0 & h_{1}\\
h_{2} & 0 & 0\\
0 & h_{2} & 0\\
0 & 0 & h_{2}\\
\vdots & \vdots & \vdots\\
h_{N} & 0 & 0\\
0 & h_{N} & 0\\
0 & 0 & h_{N}
\end{bmatrix}}_{N\times3}\underbrace{\begin{bmatrix}\sigma_{x}\parc{}x+\tau_{xy}\parc{}y+\tau_{zx}\parc{}z\\
\tau_{xy}\parc{}x+\sigma_{y}\parc{}y+\tau_{yz}\parc{}z\\
\tau_{zx}\parc{}x+\tau_{yz}\parc{}y+\sigma_{z}\parc{}z
\end{bmatrix}}_{3\times1}=\begin{bmatrix}\sigma_{x}\parc{h_{1}}x+\tau_{xy}\parc{h_{1}}y+\tau_{zx}\parc{h_{1}}z\\
\tau_{xy}\parc{h_{1}}x+\sigma_{y}\parc{h_{1}}y+\tau_{yz}\parc{h_{1}}z\\
\tau_{zx}\parc{h_{1}}x+\tau_{yz}\parc{h_{1}}y+\sigma_{z}\parc{h_{1}}z\\
\sigma_{x}\parc{h_{2}}x+\tau_{xy}\parc{h_{1}}y+\tau_{zx}\parc{h_{1}}z\\
\tau_{xy}\parc{h_{1}}x+\sigma_{y}\parc{h_{1}}y+\tau_{yz}\parc{h_{1}}z\\
\tau_{zx}\parc{h_{1}}x+\tau_{yz}\parc{h_{1}}y+\sigma_{z}\parc{h_{1}}z\\
\vdots\\
\sigma_{x}\parc{h_{1}}x+\tau_{xy}\parc{h_{1}}y+\tau_{zx}\parc{h_{1}}z\\
\tau_{xy}\parc{h_{1}}x+\sigma_{y}\parc{h_{1}}y+\tau_{yz}\parc{h_{1}}z\\
\tau_{zx}\parc{h_{1}}x+\tau_{yz}\parc{h_{1}}y+\sigma_{z}\parc{h_{1}}z
\end{bmatrix}
\]

\end_inset


